<robot name="coke_can_model">
  <link name="coke_can">
    <inertial>
      <mass value="0.1" />
      <!-- center of mass (com) is defined w.r.t. link local coordinate system -->
      <origin xyz="0 0 0.06" /> 
      <inertia  ixx="0.1" ixy="0.0"  ixz="0.0"  iyy="0.1"  iyz="0.0"  izz="0.1" />
    </inertial>
    <visual>
      <!-- visual origin is defined w.r.t. link local coordinate system -->
      <origin xyz="0 0 -0.18" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://gazebo_worlds/meshes/coke_can.dae" scale="0.001 0.001 0.001" />
      </geometry>
    </visual>
    <collision>
      <!-- collision origin is defined w.r.t. link local coordinate system -->
      <origin xyz="0 0 -0.18" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://gazebo_worlds/meshes/coke_can.dae" scale="0.001 0.001 0.001" />
      </geometry>
    </collision>
  </link>
  <gazebo reference="coke_can">
    <material>Gazebo/Red</material>
    <turnGravityOff>false</turnGravityOff>
    <kp>100000000.0</kp>
    <kd>1000000.0</kd>
    <controller:gazebo_ros_grasp_hack name="coke_can_grasp_hack_controller" plugin="libgazebo_ros_grasp_hack.so">
      <alwaysOn>true</alwaysOn>
      <updateRate>1000</updateRate>
      <graspCheckRate>10</graspCheckRate>
      <topicName>coke_can_grasp_hack</topicName>
    <controller:gazebo_ros_grasp_hack/>
  </gazebo>
</robot>
